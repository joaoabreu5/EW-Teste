extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Planta #{planta._id}
    .w3-container
      ul.w3-ul
        li Id: #{planta._id}
        li Número de Registo: #{planta['Número de Registo']}
        li Código de rua: #{planta['Código de rua']}
        li Rua: #{planta.Rua}
        li Local: #{planta.Local}
        li Freguesia: #{planta.Freguesia}
        li Espécie: #{planta.Espécie}
        li Nome Científico: #{planta['Nome Científico']}
        li Origem: #{planta.Origem}
        li Data de Plantação: #{planta['Data de Plantação']}
        li Estado: #{planta.Estado}
        li Caldeira: #{planta.Caldeira}
        li Tutor: #{planta.Tutor}
        li Implantação: #{planta.Implantação}
        li Gestor: #{planta.Gestor}
        li Data de atualização: #{planta['Data de actualização']}
        li Número de intervenções: #{planta['Número de intervenções']}

      h3
        a(href='/') Voltar à pagina inicial
    
    footer.w3-container.w3-teal
      h5 Generated by PlantasApp::EngWeb2023 on #{d}
